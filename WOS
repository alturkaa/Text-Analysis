    "df_y_hat = pd.DataFrame(model.doc_topic_, \n",
    "                        columns=topic_labels,  # Use our informative topic names\n",
    "                        index=articles.index.values) # Makes merge easier\n",
    "\n",
    "\n",
    "df_y_hat = df_y_hat.fillna(0)\n",
    "df_topics = articles.join(df_y_hat) # This is a new data set, so you can remerge"
   ]
