   "source": [
    "tf_vectorizer = CountVectorizer(max_df = 0.7,\n",
    "                                min_df = 20,\n",
    "                                stop_words='english')\n",
    "\n",
    "\n",
    "\n",
    "tf_vectorizer.fit(articles['abstract'])\n",
    "corpus_tf = tf_vectorizer.transform(articles['abstract'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 169,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "vocab = [w for w in sorted(tf_vectorizer.vocabulary_)]\n",
    "\n",
    "model = lda.LDA(n_topics= 100,\n",
    "                n_iter=500)"
   ]
  },
