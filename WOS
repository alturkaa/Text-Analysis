   "outputs": [],
   "source": [
    "def clean_ref(text):\n",
    "    shorter = text.split(',')[:2]\n",
    "    shorter = '_'.join(shorter)\n",
    "    shorter = shorter.replace('.','').replace(' ','_')\n",
    "    return shorter\n",
    "\n",
    "def make_references(text):\n",
    "    references = unicode(text).splitlines()\n",
    "    references = [clean_ref(r) for r in references]\n",
    "    return ' '.join(references)"
   ]
  },
  {
   "cell_type": "code",
